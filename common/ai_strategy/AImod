		#	ai_strategy = {
		#	type = building_target
		#	id = infrastructure
		#	value = 1000
			}
			}


#Forces CHN Upgrades to Medium tanks:
	Update_to_improvedMT = {
		Enable = {
			AND = { 
				has_war = yes
				has_tech = improved_medium_tank 
				}
				}
		abort = {
			OR = { 
				has_tech = update_med_armor 
				}
				}
			
			ai_strategy = {
			type = equipment_variant_production_factor
			id = medium_tank_equipment
			value = -100000 
			}
			}
			
	Update_to_advancedMT = {
		Enable = {
			AND = { 				
				has_tech = advanced_medium_tank 
				has_war = yes
				}
				}
		abort = {
			OR = { 
				has_tech = update_med_armor2 
				}
				}
			
			ai_strategy = {
			type = equipment_variant_production_factor
			id = medium_tank_equipment
			value = -100000 
			}
			}



