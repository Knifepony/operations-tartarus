			Update_to_advanced_infantry0 = {
		Enable = {
			AND = { 				
				has_tech = improved_infantry_weapons 
				has_war = yes
				}
				}
		abort = {
			OR = { 
				has_tech = update_infantry0 
				}
				}
			
			ai_strategy = {
			type = equipment_variant_production_factor
			id = infantry_equipment
			value = -100000 
			}
			}
	#Forces upgrades to Arty:
	
	Update_to_advanced_arty = {
		Enable = {
			AND = { 				
				has_tech = artillery1 
				has_war = yes
				}
				}
		abort = {
			OR = { 
				has_tech = update_arty1
				}
				}
			
			ai_strategy = {
			type = equipment_variant_production_factor
			id = artillery_equipment
			value = -100000 
			}
			}
			
				
