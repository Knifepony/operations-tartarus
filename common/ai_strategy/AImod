			id = modern_tank_equipment
			value = -100
 	}}
 	
 	Arti_Eq_Limit_Ult = { 
 		enable =  { 
 		OR = { 
 		has_equipment = { artillery_equipment > 10000}} 
 		}
 		abort = {
			OR { has_equipment = { artillery_equipment < 5000 }}
		}
 			
 		ai_strategy = {
			type = equipment_variant_production_factor
			id = artillery_equipment
			value = -100
 	}}
 	
