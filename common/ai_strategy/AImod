	Outwithold = {
		Enable = { 
			OR = { 
				has_tech = basic_medium_tank 
				}
				}
		abort = {
			OR = { 
				has_tech = improved_medium_tank
				has_tech = advanced_light_tank
				}
				}
				   
		ai_strategy = {
			type = equipment_variant_production_factor
			id = light_tank_equipment
			value = -25 
			}
			}


	Inwithnew = {
		Enable = {
			OR = { 
			has_tech = improved_medium_tank 
			}
			}
		abort = { 
			OR = { 
				has_tech = advanced_light_tank
				has_tech = improved_heavy_tank
				has_tech = advanced_medium_tank
				}
				}
				   
			ai_strategy = {
			type = equipment_variant_production_factor
			id = light_tank_equipment
			value = -75
			}
			
			ai_strategy = {
			type = equipment_variant_production_factor
			id = heavy_tank_equipment
			value = -100
			}
			}
			
#Increase production when ahead of curve.		
	Aheadtime = {
		Enable = {
			OR = {
				has_tech = improved_medium_tank 
				}
				}
			abort = { 
				OR = { 
					date > 1013.6.6 
					}
					}
			
			ai_strategy = {
			type = equipment_variant_production_factor
			id   = medium_tank_equipment
			value = 50 
			}
			
			ai_strategy = {
			type = role_ratio
			id = medium_armor
			value = 10
			}
			}
			
	#An attempt at creating specific AI action based on context
	Volkssturm = {
		Enable = {
			AND = { 
			surrender_progress > .2 
			has_manpower > 100000 
			}
			}
		abort = {
			OR ={ 
				has_manpower < 10000
				any_war_score > 24 
				}
				}
				
		ai_strategy = {
			type = equipment_variant_production_factor 
			id = infantry_equipment
			value = 100 
			}
		
		ai_strategy = {
			type = equipment_variant_production_factor
			id = support_equipment
			value = 50 
			}
		
		ai_strategy = {
			type = role_ratio
			id = infantry
			value = 50 
			}
			}
				   
			
			
