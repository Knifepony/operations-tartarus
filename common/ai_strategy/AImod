	#Forces upgrades to Light Tanks:
	
	Update_to_improvedLT = {
		Enable = {
			AND = { 
				has_war = yes
				has_tech = improved_light_tank 
				}
				}
		abort = {
			OR = { 
				has_tech = update_light_armor 
				}
				}
			
			ai_strategy = {
			type = equipment_variant_production_factor
			id = light_tank_equipment
			value = -100000 
			}
			}
	
	Update_to_advancedLT = {
		Enable = {
			AND = { 
				has_war = yes
				has_tech = advanced_light_tank
				}
				}
		abort = {
			OR = { 
				has_tech = update_light_armor2 }}
			
		ai_strategy = {
			type = equipment_variant_production_factor
			id = light_tank_equipment
			value = -100000 
			}
			}
			
			
	Update_to_postwarLT = {
	Enable = {
		AND = { 
			has_war = yes
			has_tech = advanced_light_tank_a 
			}
			}
		abort = {
			OR = { 
				has_war = no
				has_tech = update_light_armor3 
				}
				}
			
		ai_strategy = {
			type = equipment_variant_production_factor
			id = light_tank_equipment
			value = -100000 
			}
			}
			
			
	Update_to_modernLT = {
		Enable = {
			AND = { 
				has_war = yes
				has_tech = advanced_light_tank_b 
				}
				}
		abort = {
			OR = { 
				has_tech = update_light_armor4 
				}
				}
			
		ai_strategy = {
			type = equipment_variant_production_factor
			id = light_tank_equipment
			value = -10000 
			}
			}
