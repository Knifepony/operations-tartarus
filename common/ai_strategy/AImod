#Scaling down Ai's desire for ships because they are mostly a waste of their resources.
	
	Less_Ships_for_all = {
		enable = {
			OR = {
				always = yes
				}
				}
		Abort = {
			OR = {
				always = no 
				}
				}
			
		ai_strategy = {
			type = unit_ratio
			id = capital_ship
			value = -50
			}

		ai_strategy = {
			type = unit_ratio
			id = submarine
			value = -50
			}

		ai_strategy = {
			type = unit_ratio
			id = screen_ship
			value = -30
			}

		ai_strategy = {
			type = unit_ratio
			id = convoy
			value = -50
			}
		ai_strategy = {
		type = dockyard_to_military_factory_ratio
		value = -45	
			}
			
